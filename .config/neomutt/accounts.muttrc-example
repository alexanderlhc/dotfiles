unmailboxes *

set from      = "mail@mail.com"
set sendmail  = "/usr/bin/msmtp -a mail.com"
set folder = "~/Mail/mail.com/"
set spoolfile = "+INBOX"
set postponed = "+Drafts"
set record = "+Sent"
set trash = "+Junk"

alias me "First Last" "mail@mail.com"

set mbox_type = Maildir
set ssl_starttls = yes
set ssl_force_tls = yes

mailboxes =INBOX =Sent =Drafts =Junk =Trash =Archive
